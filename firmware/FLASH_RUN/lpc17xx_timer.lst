  57:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c ****  * @brief 		Get peripheral clock of each timer controller
  58:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c ****  * @param[in]	timernum Timer number
  59:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c ****  * @return 		Peripheral clock of timer
  60:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c ****  **********************************************************************/
  61:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** static uint32_t getPClock (uint32_t timernum)
  62:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** {
  63:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	uint32_t clkdlycnt;
  64:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	switch (timernum)
  65:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	{
  66:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	case 0:
  67:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 		clkdlycnt = CLKPWR_GetPCLK (CLKPWR_PCLKSEL_TIMER0);
  68:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 		break;
  69:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 
  70:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	case 1:
  71:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 		clkdlycnt = CLKPWR_GetPCLK (CLKPWR_PCLKSEL_TIMER1);
  72:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 		break;
  73:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 
  74:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	case 2:
  75:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 		clkdlycnt = CLKPWR_GetPCLK (CLKPWR_PCLKSEL_TIMER2);
  76:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 		break;
  77:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 
  78:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	case 3:
  79:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 		clkdlycnt = CLKPWR_GetPCLK (CLKPWR_PCLKSEL_TIMER3);
  80:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 		break;
  81:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	}
  82:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	return clkdlycnt;
  83:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** }
