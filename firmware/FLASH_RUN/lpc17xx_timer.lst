 105:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c ****  * @brief 		Convert a timer register pointer to a timer number
 106:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c ****  * @param[in]	TIMx Pointer to LPC_TIM_TypeDef, should be:
 107:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c ****  * 				- LPC_TIM0: TIMER0 peripheral
 108:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c ****  * 				- LPC_TIM1: TIMER1 peripheral
 109:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c ****  * 				- LPC_TIM2: TIMER2 peripheral
 110:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c ****  * 				- LPC_TIM3: TIMER3 peripheral
 111:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c ****  * @return 		The timer number (0 to 3) or -1 if register pointer is bad
 112:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c ****  **********************************************************************/
 113:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** uint32_t converPtrToTimeNum (LPC_TIM_TypeDef *TIMx)
 114:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** {
 115:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	uint32_t tnum = -1;
 116:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 
 117:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	if (TIMx == LPC_TIM0)
 118:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	{
 119:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 		tnum = 0;
 120:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	}
 121:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	else if (TIMx == LPC_TIM1)
 122:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	{
 123:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 		tnum = 1;
 124:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	}
 125:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	else if (TIMx == LPC_TIM2)
 126:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	{
 127:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 		tnum = 2;
 128:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	}
 129:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	else if (TIMx == LPC_TIM3)
 130:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	{
 131:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 		tnum = 3;
 132:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	}
 133:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 
 134:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	return tnum;
 135:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** }
 136:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 
