  87:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c ****  * @brief 		Convert a time to a timer count value
  88:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c ****  * @param[in]	timernum Timer number
  89:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c ****  * @param[in]	usec Time in microseconds
  90:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c ****  * @return 		The number of required clock ticks to give the time delay
  91:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c ****  **********************************************************************/
  92:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** uint32_t converUSecToVal (uint32_t timernum, uint32_t usec)
  93:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** {
  94:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	uint64_t clkdlycnt;
  95:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 
  96:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	// Get Pclock of timer
  97:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	clkdlycnt = (uint64_t) getPClock(timernum);
  98:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 
  99:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	clkdlycnt = (clkdlycnt * usec) / 1000000;
 100:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** 	return (uint32_t) clkdlycnt;
 101:src/MightyBoard/Motherboard/lib/source/lpc17xx_timer.c **** }
