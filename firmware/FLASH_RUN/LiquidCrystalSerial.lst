  53:src/MightyBoard/shared/LiquidCrystalSerial.cc **** void LiquidCrystalSerial::begin(uint8_t cols, uint8_t lines, uint8_t dotsize) {
  54:src/MightyBoard/shared/LiquidCrystalSerial.cc ****   if (lines > 1) {
  55:src/MightyBoard/shared/LiquidCrystalSerial.cc ****     _displayfunction |= LCD_2LINE;
  56:src/MightyBoard/shared/LiquidCrystalSerial.cc ****   }
  57:src/MightyBoard/shared/LiquidCrystalSerial.cc ****   _numlines = lines;
  58:src/MightyBoard/shared/LiquidCrystalSerial.cc ****   _numCols = cols;
  59:src/MightyBoard/shared/LiquidCrystalSerial.cc **** 
  60:src/MightyBoard/shared/LiquidCrystalSerial.cc ****   // for some 1 line displays you can select a 10 pixel high font
  61:src/MightyBoard/shared/LiquidCrystalSerial.cc ****   if ((dotsize != 0) && (lines == 1)) {
  62:src/MightyBoard/shared/LiquidCrystalSerial.cc ****     _displayfunction |= LCD_5x10DOTS;
  63:src/MightyBoard/shared/LiquidCrystalSerial.cc ****   }
  64:src/MightyBoard/shared/LiquidCrystalSerial.cc **** 
  65:src/MightyBoard/shared/LiquidCrystalSerial.cc ****   // SEE PAGE 45/46 FOR INITIALIZATION SPECIFICATION!
  66:src/MightyBoard/shared/LiquidCrystalSerial.cc ****   // according to datasheet, we need at least 40ms after power rises above 2.7V
  67:src/MightyBoard/shared/LiquidCrystalSerial.cc ****   // before sending commands. Arduino can turn on way befer 4.5V so we'll wait 50
  68:src/MightyBoard/shared/LiquidCrystalSerial.cc ****   _delay_us(50000);
  69:src/MightyBoard/shared/LiquidCrystalSerial.cc ****   // Now we pull both RS and R/W low to begin commands
  70:src/MightyBoard/shared/LiquidCrystalSerial.cc ****   writeSerial(0b00000000);
  71:src/MightyBoard/shared/LiquidCrystalSerial.cc ****   
  72:src/MightyBoard/shared/LiquidCrystalSerial.cc ****   //put the LCD into 4 bit or 8 bit mode
  73:src/MightyBoard/shared/LiquidCrystalSerial.cc ****   if (! (_displayfunction & LCD_8BITMODE)) {
  74:src/MightyBoard/shared/LiquidCrystalSerial.cc ****     // this is according to the hitachi HD44780 datasheet
  75:src/MightyBoard/shared/LiquidCrystalSerial.cc ****     // figure 24, pg 46
  76:src/MightyBoard/shared/LiquidCrystalSerial.cc **** 
  77:src/MightyBoard/shared/LiquidCrystalSerial.cc ****     // we start in 8bit mode, try to set 4 bit mode
  78:src/MightyBoard/shared/LiquidCrystalSerial.cc ****     load(0x03 << 4);
  79:src/MightyBoard/shared/LiquidCrystalSerial.cc ****     _delay_us(4500); // wait min 4.1ms
  80:src/MightyBoard/shared/LiquidCrystalSerial.cc **** 
  81:src/MightyBoard/shared/LiquidCrystalSerial.cc ****     // second try
  82:src/MightyBoard/shared/LiquidCrystalSerial.cc ****     load(0x03 << 4);
  83:src/MightyBoard/shared/LiquidCrystalSerial.cc ****     _delay_us(4500); // wait min 4.1ms
  84:src/MightyBoard/shared/LiquidCrystalSerial.cc ****     
  85:src/MightyBoard/shared/LiquidCrystalSerial.cc ****     // third go!
  86:src/MightyBoard/shared/LiquidCrystalSerial.cc ****     load(0x03 << 4); 
  87:src/MightyBoard/shared/LiquidCrystalSerial.cc ****     _delay_us(150);
  88:src/MightyBoard/shared/LiquidCrystalSerial.cc **** 
  89:src/MightyBoard/shared/LiquidCrystalSerial.cc ****     // finally, set to 8-bit interface
  90:src/MightyBoard/shared/LiquidCrystalSerial.cc ****     load(0x02 << 4); 
  91:src/MightyBoard/shared/LiquidCrystalSerial.cc ****   } else {
  92:src/MightyBoard/shared/LiquidCrystalSerial.cc ****     // this is according to the hitachi HD44780 datasheet
  93:src/MightyBoard/shared/LiquidCrystalSerial.cc ****     // page 45 figure 23
  94:src/MightyBoard/shared/LiquidCrystalSerial.cc **** 
