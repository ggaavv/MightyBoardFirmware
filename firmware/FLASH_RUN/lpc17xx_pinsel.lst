  97:src/MightyBoard/Motherboard/lib/source/lpc17xx_pinsel.c **** 	uint32_t pinselreg_idx = 2 * portnum;
  98:src/MightyBoard/Motherboard/lib/source/lpc17xx_pinsel.c **** 	uint32_t *pPinCon = (uint32_t *)&LPC_PINCON->PINSEL0;
  99:src/MightyBoard/Motherboard/lib/source/lpc17xx_pinsel.c **** 
 100:src/MightyBoard/Motherboard/lib/source/lpc17xx_pinsel.c **** 	if (pinnum_t >= 16) {
 101:src/MightyBoard/Motherboard/lib/source/lpc17xx_pinsel.c **** 		pinnum_t -= 16;
 102:src/MightyBoard/Motherboard/lib/source/lpc17xx_pinsel.c **** 		pinselreg_idx++;
 103:src/MightyBoard/Motherboard/lib/source/lpc17xx_pinsel.c **** 	}
 104:src/MightyBoard/Motherboard/lib/source/lpc17xx_pinsel.c **** 	*(uint32_t *)(pPinCon + pinselreg_idx) &= ~(0x03UL << (pinnum_t * 2));
 105:src/MightyBoard/Motherboard/lib/source/lpc17xx_pinsel.c **** 	*(uint32_t *)(pPinCon + pinselreg_idx) |= ((uint32_t)funcnum) << (pinnum_t * 2);
